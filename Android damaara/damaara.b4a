		Try
			If Application.VersionCode > File.ReadString(SaeloZahra.dir,"VAPP") Then
				NewVersion = True
			End If
		Catch
			File.WriteString(SaeloZahra.dir,"VAPP",Application.VersionCode)
		End Try
		File.WriteString(SaeloZahra.dir,"VAPP",Application.VersionCode)
		
		
		If Not(File.Exists(SaeloZahra.dir,"db.db")) Then
			File.Copy(File.DirAssets,"db.db",SaeloZahra.dir,"db.db")
		Else If NewVersion Then
			Msgbox2Async("آیا قصد حذف آرشیو اطلاعات و پیام‌ها را دارید؟", "پاکسازی", "آره ، حذفشون کن", "", "نه، میخوام داشته باشمشون", Null, False)
